<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="klp.com.animationdemo">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MediaActivity"></activity>
        <activity android:name=".BesselActivity" />
        <activity android:name=".CommentActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ButtonActivity"
            android:label="@string/button_name" />
        <activity
            android:name=".AnimationViewActivity"
            android:label="@string/animation_view_name" />
        <activity android:name=".SharedElementsActivity" />
        <activity
            android:name=".SwipeRefreshActivity"
            android:label="@string/swipe_refresh_activity"
            android:theme="@style/AppTheme.NestedScroll" />
        <activity android:name=".CoordinatorSimpleActivity" />
        <activity
            android:name=".BehaviorActivity"
            android:label="@string/custom_nested_scroll" />
        <activity android:name=".Behavior2Activity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".CustomProgressActivity" />
        <activity android:name=".MoveAbleActivity" />
        <activity android:name=".MoveAbleActivity2" />
        <activity android:name=".BottomNavigationActivity" />
        <activity android:name=".AppcompatDialogActivity" />
        <activity android:name=".CheckBoxActivity" />
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".BehaviorLayoutDependsOnActivity" />
        <activity android:name=".Rx.RxActivity" />
        <activity
            android:name=".NestedScrollActivity"
            android:theme="@style/AppTheme.NestedScroll" />
        <activity android:name=".AdaptationActivity" />
        <activity android:name=".CameraActivity" />
        <activity android:name=".SemicircleActivity" />

        <service
            android:name=".media.PlayService"
            android:enabled="true"
            android:exported="false" />
    </application>

</manifest>